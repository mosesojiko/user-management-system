<!--Display axios result in this form-->
<% for(var i=0; i < users.length; i++){ %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <!--link to update a user-->
        <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <!--button to delete a user-->
        <a class="btn border-shadow delete" data-id=<%=users[i]._id %> >
            <span class="text-gradient"><i class="fas fa-times" aria-hidden="true"></i></span>
        </a>
    </td>
</tr>
<% } %>
